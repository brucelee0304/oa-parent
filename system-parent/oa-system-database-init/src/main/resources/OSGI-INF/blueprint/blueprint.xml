<blueprint xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
           xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd">

    <reference id="applicationService" interface="com.kalix.admin.core.api.biz.IApplicationBeanService"/>
    <reference id="functionService" interface="com.kalix.admin.core.api.biz.IFunctionBeanService"/>
    <bean id="DBInit" class="com.kalix.oa.system.database.DBInit" init-method="init">
        <property name="applicationService" ref="applicationService"/>
        <property name="functionService" ref="functionService"/>
    </bean>
</blueprint>

