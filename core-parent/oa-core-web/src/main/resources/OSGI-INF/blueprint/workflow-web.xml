<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
           xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd">
    <!--办公自动化应用-->
    <service interface="com.kalix.framework.core.api.web.IApplication">
        <bean class="com.kalix.oa.core.web.impl.app.OAApplicationImpl"/>
    </service>

    <!--流程管理模块-->
    <service interface="com.kalix.framework.core.api.web.IModule">
        <bean class="com.kalix.oa.core.web.impl.module.WorkFlowModuleImpl"/>
    </service>
    <!--流程消息模块-->
    <service interface="com.kalix.framework.core.api.web.IModule">
        <bean class="com.kalix.oa.core.web.impl.module.MyMessageModuleImpl"/>
    </service>

    <!--业务流程模块-->
    <service interface="com.kalix.framework.core.api.web.IModule">
        <bean class="com.kalix.oa.core.web.impl.module.WorkFlowBizModuleImpl"/>
    </service>

    <!--我的流程模块-->
    <service interface="com.kalix.framework.core.api.web.IModule">
        <bean class="com.kalix.oa.core.web.impl.module.MyWorkingModuleImpl"/>
    </service>


    <!--流程定义管理-->
    <service interface="com.kalix.framework.core.api.web.IMenu">
        <bean class="com.kalix.oa.core.web.impl.menu.ProcessDefinitionMenuImpl"/>
    </service>

    <!--待办任务列表-->
    <service interface="com.kalix.framework.core.api.web.IMenu">
        <bean class="com.kalix.oa.core.web.impl.menu.TaskMenuImpl"/>
    </service>

    <!--流程历史列表-->
    <service interface="com.kalix.framework.core.api.web.IMenu">
        <bean class="com.kalix.oa.core.web.impl.menu.ProcessHistoryMenuImpl"/>
    </service>

    <!-- 我的流程历史列表-->
    <service interface="com.kalix.framework.core.api.web.IMenu">
        <bean class="com.kalix.oa.core.web.impl.menu.MyProcessHistoryMenuImpl"/>
    </service>
</blueprint>